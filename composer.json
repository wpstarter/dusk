{
    "name": "wpstarter/dusk",
    "description": "WpStarter Dusk provides simple end-to-end testing and browser automation.",
    "keywords": ["wpstarter", "testing", "webdriver"],
    "license": "MIT",
    "authors": [
        {
            "name": "As247",
            "email": "as247@wpstarter.dev"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "ext-zip": "*",
        "php-webdriver/webdriver": "^1.9.0",
        "nesbot/carbon": "^2.0",
        "wp-starter/console": "^1.0",
        "wp-starter/support": "^1.0",
        "symfony/console": "^4.3|^5.0|^6.0",
        "symfony/finder": "^4.3|^5.0|^6.0",
        "symfony/process": "^4.3|^5.0|^6.0",
        "vlucas/phpdotenv": "^3.0|^4.0|^5.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5.15|^8.4|^9.0",
        "wpstarter/o-testbench": "^1.0"
    },
    "suggest": {
        "ext-pcntl": "Used to gracefully terminate Dusk when tests are running."
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Dusk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Dusk\\Tests\\": "tests/"
        }
    },
    "extra": {
        "wpstarter": {
            "providers": [
                "Laravel\\Dusk\\DuskServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
